[Interface]
Address = @@INT_CIDR@@
PrivateKey = @@INT_PRIVATEKEY@@

[Peer]
PublicKey = @@PEER_PUBLICKEY@@
PresharedKey = @@PEER_PRESHAREDKEY@@
AllowedIPs = 0.0.0.0/0
Endpoint = @@PEER_ENDPOINT@@:@@PEER_PORT@@
PersistentKeepalive = 25

